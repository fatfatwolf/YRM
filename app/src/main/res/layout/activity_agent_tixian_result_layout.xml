<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#fff"
    android:orientation="vertical">

    <com.hybunion.yirongma.payment.view.TitleBar
        android:id="@+id/titleBar_tixian_result_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:titlebar_back_title="提现结果">

    </com.hybunion.yirongma.payment.view.TitleBar>

    <ImageView
        android:layout_width="109px"
        android:layout_height="81px"
        app:layout_auto_basewidth="height"
        android:src="@drawable/img_pay_success"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="28px"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="提现申请已提交，等待银行处理"
        android:textSize="15px"
        android:textColor="@color/title_text_color"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="29px"/>


    <TextView
        android:id="@+id/finish_tixian_result_activity"
        android:layout_width="140px"
        android:layout_height="35px"
        app:layout_auto_basewidth="height"
        android:text="完成"
        android:textColor="#f74948"
        android:layout_marginTop="85px"
        android:layout_marginBottom="41px"
        android:textSize="15px"
        android:layout_gravity="center_horizontal"
        android:gravity="center"
        android:background="@drawable/shape_tixian_result"/>


</LinearLayout>