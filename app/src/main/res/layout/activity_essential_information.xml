        <!--</RelativeLayout>-->

        <!--<View style="@style/view_divider" />-->


        <View style="@style/view_line10" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="账户名称"
                android:textColor="@color/text_level_one"
                android:textSize="@dimen/text_level_1" />

            <TextView
                android:id="@+id/info_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:textColor="@color/text_level_two"
                android:textSize="@dimen/text_level_2" />

        </RelativeLayout>

        <View style="@style/view_divider" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="结算账户"
                android:textColor="@color/text_level_one"
                android:textSize="@dimen/text_level_1" />

            <TextView
                android:id="@+id/info_6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:textColor="@color/text_level_two"
                android:textSize="@dimen/text_level_2" />

        </RelativeLayout>

        <View style="@style/view_divider" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="结算账户银行"
                android:textColor="@color/text_level_one"
                android:textSize="@dimen/text_level_1" />

            <TextView
                android:id="@+id/info_7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:textColor="@color/text_level_two"
                android:textSize="@dimen/text_level_2" />

        </RelativeLayout>

        <View style="@style/view_divider" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:gravity="center_vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="注册身份证号"
                android:textColor="@color/text_level_one"
                android:textSize="@dimen/text_level_1" />

            <TextView
                android:id="@+id/info_8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:textColor="@color/text_level_two"
                android:textSize="@dimen/text_level_2" />

        </RelativeLayout>

        <View style="@style/view_line10" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
